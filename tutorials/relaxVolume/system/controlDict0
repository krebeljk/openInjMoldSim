FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     openInjMoldDyMSimFimabEab;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         0.14;

deltaT          1e-7;

writeControl    adjustableRunTime;

writeInterval   0.014;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   10;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.2;

maxDeltaT       1;

maxAlphaCo      0.15;
libs ("libextWHFTP.so");
functions {

    probes
    {
        type            probes; //patchProbes;//
        functionObjectLibs ("libsampling.so");
        outputControl   timeStep;
        outputInterval  10;
        probeLocations
        (
         (10e-3 0.05e-3 0.605e-3)
         (37.9e-3 0.05e-3 0.605e-3)
         (66.25e-3 0.05e-3 0.605e-3)
        );
        fields
        (
            Tc p rho
            U elSigDev shrRate visc
            vf
        );
    }

    probeTtP2
    {
        type            probes; //patchProbes;//
        functionObjectLibs ("libsampling.so");
        outputControl   timeStep;
        outputInterval  10;
        fixedLocations  false;// false = move points with mesh
        probeLocations
        (
         #include "$FOAM_CASE/tt_pts.txt"
        );
        fields
        (
            Tc p rho
            U elSigDev shrRate visc
            vf
        );
    }


        residuals
        {
            type           residuals;
            functionObjectLibs ("libutilityFunctionObjects.so");
            enabled        true;
            writeControl   timeStep;
            writeInterval  1;
            fields
            (
                p_rgh
                T
            );
        }
    flowRatePatchIn
    {
        patch inlet;

        #includeEtc "caseDicts/postProcessing/faceSource/patchSource.cfg"
        fields (phi rhoPhi);
        operation sum;
    }
    flowRatePatchOut
    {
        patch outlet;

        #includeEtc "caseDicts/postProcessing/faceSource/patchSource.cfg"
        fields (phi rhoPhi);
        operation sum;
    }
}

