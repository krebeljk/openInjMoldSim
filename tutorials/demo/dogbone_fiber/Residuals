#!/usr/bin/env gnuplot

set multiplot layout 3, 1 title "Residuals"

set style line 1 lc rgb '#0060ad' lt 1 lw 1 pt 7 ps 0 # --- blue
set style line 2 lc rgb '#9fac43' lt 1 lw 1 pt 7 ps 0 # --- blue
set style line 3 lc rgb '#dd181f' lt 1 lw 1 pt 7 ps 0 # --- blue
set style line 4 lc rgb '#000000' lt 2 lw 1 pt 7 ps 0 # --- blue

set logscale y
set format y "1e{%L}"
set title "p rgh"
set ylabel 'Residual'
set xlabel 'Iteration'
plot "< grep 'GAMG:' log.openInjMoldSim | cut -d' ' -f9 | tr -d ','" title 'GAMG' with lines ls 1

set logscale y
set format y "1e{%L}"
set title "T"
set ylabel 'Residual'
set xlabel 'Iteration'
plot "< grep 'smoothSolver:  Solving for T,' log.openInjMoldSim | cut -d' ' -f9 | tr -d ','" title 'smoothSolver T ' with lines ls 1

set logscale y
set format y "1e{%L}"
set title "elSigDev"
set ylabel 'Residual'
set xlabel 'Iteration'
plot "< grep 'smoothSolver:  Solving for elSigDevxx, Initial residual =' log.openInjMoldSim | cut -d' ' -f9 | tr -d ','" using 0:(($1==0) ? 1.1 : $1) title 'elSigDevxx' with lines ls 1,\
"< grep 'smoothSolver:  Solving for elSigDevyy, Initial residual =' log.openInjMoldSim | cut -d' ' -f9 | tr -d ','" using 0:(($1==0) ? 1.1 : $1) title 'elSigDevyy' with lines ls 2,\
"< grep 'smoothSolver:  Solving for elSigDevzz, Initial residual =' log.openInjMoldSim | cut -d' ' -f9 | tr -d ','" using 0:(($1==0) ? 1.1 : $1) title 'elSigDevzz' with lines ls 3 ,\
"< grep 'smoothSolver:  Solving for elSigDevxz, Initial residual =' log.openInjMoldSim | cut -d' ' -f9 | tr -d ','" using 0:(($1==0) ? 1.1 : $1) title 'elSigDevxz' with lines ls 4
pause 5
reread
